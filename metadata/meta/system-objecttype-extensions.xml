      "custom-attribute-definitions": 
        "attribute-definition": 
          - "@attributes": 
              "attribute-id": alternativeUrl
            "display-name": "Alternative URL"
            description: "Renders an alternative URL in main navigation. Uses Commerce Cloud content url notation. For example: \x24url\x28\x27Account-Show\x27\x29\x24 or normal URL http://xchange.demandware.com"
            type: html
            "localizable-flag": false
            "mandatory-flag": false
            "externally-managed-flag": false
            "field-length": 0
            "field-height": 2
          - "@attributes": 
              "attribute-id": catBannerID
            "display-name": "Category Banner ID"
            description: "Used to define the content asset used to populate a grid page banner for a category. This value is applied to all sub-category navigation \x28cascading\x29 if no specific catBannerID has been defined for  a sub-category."
            type: string
            "localizable-flag": false
            "mandatory-flag": false
            "externally-managed-flag": false
            "min-length": 0
          - "@attributes": 
              "attribute-id": customCSSFile
            "display-name": "Custom CSS File"
            description: "Use this attribute to apply custom styles for this category."
            type: image
            "localizable-flag": false
            "mandatory-flag": false
            "externally-managed-flag": false
          - "@attributes": 
              "attribute-id": enableCompare
            "display-name": "Enable Compare"
            description: "Used to define if/when the Compare feature is to be visualized in the storefront based on navigation. If enableCompare \x3D FALSE\x2C no Compare checkboxes will be displayed in the grid view. If enableCompare \x3D TRUE\x2C the category \x28and its children\x29 will support the Compare feature."
            type: boolean
            "mandatory-flag": false
            "externally-managed-flag": false
          - "@attributes": 
              "attribute-id": headerMenuBanner
            "display-name": "Header Menu Banner"
            type: html
            "localizable-flag": false
            "mandatory-flag": false
            "externally-managed-flag": false
          - "@attributes": 
              "attribute-id": headerMenuOrientation
            "display-name": "Header Menu Orientation"
            description: "Which way to orient the menu and optional header menu HTML. Vertical will list all in one line. Horizontal will list in columns."
            type: "enum-of-string"
            "localizable-flag": false
            "mandatory-flag": false
            "externally-managed-flag": false
            "value-definitions": 
              "value-definition": 
                - "@attributes": 
                    default: true
                  display: Horizontal
                  value: Horizontal
                - display: Vertical
                  value: Vertical
          - "@attributes": 
              "attribute-id": showInMenu
            "display-name": "Show in Menu Navigation"
            description: "Used to indicate that a category \x28such as Mens -\x3E Footwear -\x3E Boots\x29 will display in the roll-over navigation. A sub-category only shows if also the parent category is marked as showInMenu. Up to three category levels are shown in roll-over navigation."
            type: boolean
            "mandatory-flag": false
            "externally-managed-flag": false
          - "@attributes": 
              "attribute-id": sizeChartID
            "display-name": "Size Chart"
            description: "Used to define the content asset ID of the Size Chart that is appropriate for products whose PRIMARY category is the associated category \x28and its children\x29. Whenever a product detail page \x28or quick view\x29 is rendered\x2C the Size Chart link is populated based on the value of this attribute for the products primary categorization. If not defined\x2C NO size chart link is displayed."
            type: string
            "localizable-flag": false
            "mandatory-flag": false
            "externally-managed-flag": false
            "min-length": 0
          - "@attributes": 
              "attribute-id": slotBannerHtml
            "display-name": "Slot Banner HTML"
            type: html
            "localizable-flag": true
            "mandatory-flag": false
            "externally-managed-flag": false
          - "@attributes": 
              "attribute-id": slotBannerImage
            "display-name": "Category Landing Slot Image"
            description: "Image used on either the top or bottom slot on the category landing pages."
            type: image
            "localizable-flag": false
            "mandatory-flag": false
            "externally-managed-flag": false
      "group-definitions": 
        "attribute-group": 
          - "@attributes": 
              "group-id": ExternalSearch
            "display-name": "Search Engine Optimization Support"
            attribute: 
